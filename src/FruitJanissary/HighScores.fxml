<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FruitJanissary.HighScoresController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/HighScoreScreen.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="729.0" layoutY="395.0" mnemonicParsing="false" onMouseClicked="#onBackClicked" prefHeight="89.0" prefWidth="139.0" style="-fx-background-color: transparent;" />
      <TableView fx:id="list" layoutX="57.0" layoutY="306.0" prefHeight="269.0" prefWidth="636.0" />
      <CheckBox fx:id="ownHighScores" layoutX="703.0" layoutY="307.0" mnemonicParsing="false" text="Show my high scores" textFill="#f2f2f2">
         <font>
            <Font name="Comic Sans MS Bold" size="15.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
